{
    "manifest_version": 3,
    "name": "Nswer",
    "version": "1.0.0",
    "description": "Ask and you shall receive!",
    "permissions": [
        "activeTab"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "host_permissions":[
        "http://localhost:3000/*"
    ]
  }